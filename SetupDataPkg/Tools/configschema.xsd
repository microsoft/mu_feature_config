<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema">
  <xs:element name="ConfigSchema">
    <xs:complexType>
      <xs:sequence>
        <xs:element name="Enums">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="Enum">
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="EnumValue" maxOccurs="unbounded">
                      <xs:complexType>
                        <xs:attribute name="name" use="required" />
                        <xs:attribute name="value" type="xs:integer" use="required" />
                        <xs:attribute name="help" use="required" />
                        <xs:attribute name="deprecated" type="xs:boolean" use="optional" />
                      </xs:complexType>
                    </xs:element>
                  </xs:sequence>
                  <xs:attribute name="name" use="required" />
                  <xs:attribute name="help" use="optional" />
                </xs:complexType>
              </xs:element>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
        <xs:element name="Knobs" maxOccurs="1">
          <xs:complexType>
            <xs:sequence>
              <xs:choice maxOccurs="unbounded">
                <xs:element name="EnumKnob" maxOccurs="unbounded">
                  <xs:complexType>
                    <xs:attribute name="name" use="required" />
                    <xs:attribute name="enumtype" use="required" />
                    <xs:attribute name="default" use="required" />
                    <xs:attribute name="help" use="optional" />
                    <xs:attribute name="deprecated" type="xs:boolean" use="optional" />
                  </xs:complexType>
                </xs:element>
                <xs:element name="FloatKnob" maxOccurs="unbounded">
                  <xs:complexType>
                    <xs:attribute name="name" use="required" />
                    <xs:attribute name="default" type="xs:decimal" use="required" />
                    <xs:attribute name="help" use="optional" />
                    <xs:attribute name="deprecated" type="xs:boolean" use="optional" />
                  </xs:complexType>
                </xs:element>
                <xs:element name="DoubleKnob" maxOccurs="unbounded">
                  <xs:complexType>
                    <xs:attribute name="name" use="required" />
                    <xs:attribute name="default" type="xs:decimal" use="required" />
                    <xs:attribute name="help" use="optional" />
                    <xs:attribute name="deprecated" type="xs:boolean" use="optional" />
                  </xs:complexType>
                </xs:element>
                <xs:element name="Int32Knob">
                  <xs:complexType>
                    <xs:attribute name="name" use="required" />
                    <xs:attribute name="default" type="xs:integer" use="required" />
                    <xs:attribute name="help" use="optional" />
                    <xs:attribute name="deprecated" type="xs:boolean" use="optional" />
                  </xs:complexType>
                </xs:element>
                <xs:element name="Int64Knob">
                  <xs:complexType>
                    <xs:attribute name="name" use="required" />
                    <xs:attribute name="default" type="xs:integer" use="required" />
                    <xs:attribute name="help" use="optional" />
                    <xs:attribute name="deprecated" type="xs:boolean" use="optional" />
                  </xs:complexType>
                </xs:element>
                <xs:element name="BytesKnob">
                  <xs:complexType>
                    <xs:attribute name="name" use="required" />
                    <xs:attribute name="default" type="xs:hexBinary" use="required" />
                    <xs:attribute name="help" use="optional" />
                    <xs:attribute name="deprecated" type="xs:boolean" use="optional" />
                  </xs:complexType>
                </xs:element>
                <xs:element name="BoolKnob">
                  <xs:complexType>
                    <xs:attribute name="name" use="required" />
                    <xs:attribute name="default" type="xs:boolean" use="required" />
                    <xs:attribute name="help" use="optional" />
                    <xs:attribute name="deprecated" type="xs:boolean" use="optional" />
                  </xs:complexType>
                </xs:element>
              </xs:choice>
            </xs:sequence>
            <xs:attribute name="namespace" use="required" />
          </xs:complexType>
        </xs:element>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
</xs:schema>